{
  "name": "solvion-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,html}\"",
    "type-check": "tsc --noEmit",
    "check:all": "npm run format:check && npm run lint && npm run type-check && npm audit --audit-level high"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.13",
    "immer": "10.1.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.9.1",
    "tailwindcss": "4.1.13",
    "zod": "4.1.9",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/node": "24.5.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react-swc": "4.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.5"
  }
}
