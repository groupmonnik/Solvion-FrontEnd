{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "useTabs": false,
  "endOfLine": "lf",
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
